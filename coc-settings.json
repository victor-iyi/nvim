{
  // Python linting
  "python.linting.enabled": true,
  "python.linting.pydocstyleArgs": [
      "--ignore E251"
  ],
  // Rust Analyzer
  "rust-analyzer.enable":  true,
  "rust-analyzer.cargo.target": "aarch64-apple-darwin",
  // Lua
  "Lua.telemetry.enable": true,
  "snippets.ultisnips.pythonPrompt": false
}
